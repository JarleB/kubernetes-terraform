apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority-data: ${ca_base64}
    server: https://${apiserver_address}/
  name: ${cluster_name}
users:
- name: ${cluster_name}-admin
  user:
    client-certificate-data: ${kube_admin_cert_base64}
    client-key-data: ${kube_admin_key_base64}
contexts:
- context:
    cluster: ${cluster_name}
    user: ${cluster_name}-admin
  name: ${cluster_name}-admin
current-context: ${cluster_name}-admin
